{
  "extends": "@vue/tsconfig/tsconfig.dom.json",
  // "include": ["env.d.ts", "src/**/*", "src/**/*.vue", "src/**/*.json", "src/**/*.ts", "typings"],
  "exclude": ["src/**/__tests__/*"],
  "compilerOptions": {
    "composite": true,
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"]
    },
    "moduleResolution": "node",
    "target": "esnext",
    "module": "esnext",
    "verbatimModuleSyntax": false
  }
}
